		]]></listrepeat>
		<expres>(\d\d:\d\d)\s+(.+?)\|\s+https(.+?)\.php</expres>
		<page>https://sportzonline.to/prog.txt</page>
</regex>
<regex>
        <name>geturl</name>
        <expres>Clappr.Player[\w\W]*?(?:source|src|file):\s*[\'"]([^\'"]+)</expres>
        <page>$doregex[getunpacked]</page>
