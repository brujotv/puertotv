def GetLSProData(page_data,Cookie_Jar,m):
  import json
  channels = []
  items = json.loads(page_data)
  for item in items:
    title = item.get('name')
    cid = item.get('content_id')
    info = item.get('description')
    channels.append((title, cid, info))
  return sorted(channels)
]]></expres>
